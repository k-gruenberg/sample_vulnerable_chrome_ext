{
  "manifest_version": 3,
  "name": "A non-vulnerable Chrome Extension",
  "version": "1.0",
  "description": "A simple, non-vulnerable, Chrome extension",
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
